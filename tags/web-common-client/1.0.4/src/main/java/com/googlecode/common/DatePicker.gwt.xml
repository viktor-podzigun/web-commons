<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/2.4/distro-source/core/src/gwt-module.dtd">
<module>
    <!-- Inherit Client module -->
    <inherits name='com.googlecode.common.Ui' />

    <!-- Specify the paths for translatable code -->
    <source path="client/datepicker"/>

    <!-- Specify the public paths -->
    <public path="res" />
    
    <stylesheet src="datepicker/css/datetimepicker.min.css" />
    
    <script src="datepicker/js/datetimepicker.min.js" />
    
</module>
