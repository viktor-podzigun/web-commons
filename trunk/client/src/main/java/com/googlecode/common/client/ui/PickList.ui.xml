<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
             xmlns:c="urn:import:com.googlecode.common.client.ui">

  <g:HTMLPanel styleName="row-fluid">
      <div class="span5">
        <strong><c:ImageLabel ui:field="sourceLabel" text="Available items:" /></strong>
        <hr />
        <p1:CellList ui:field="sourceList" />
      </div>

      <div class="btn-group-vertical span2">
        <g:Button ui:field="addButton" styleName="btn" width="35px" enabled="false">
          &gt;</g:Button>
        <g:Button ui:field="removeButton" styleName="btn" width="35px" enabled="false">
          &lt;</g:Button>
        <g:Button ui:field="addAllButton" styleName="btn" width="35px" enabled="false">
          &gt;&gt;</g:Button>
        <g:Button ui:field="removeAllButton" styleName="btn" width="35px" enabled="false">
          &lt;&lt;</g:Button>
      </div>

      <div class="span5">
        <strong><c:ImageLabel ui:field="destLabel" text="Selected items:" /></strong>
        <hr />
        <p1:CellList ui:field="destList" />
      </div>
  </g:HTMLPanel>

</ui:UiBinder>
